<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mini.timetable"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="24" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.mini.timetable.Menu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mini.timetable.timetable"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.sv.myfirstproject.timetable" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mini.timetable.Day1"
            android:label="@string/title_activity_day1" >
        </activity>
        <activity
            android:name="com.mini.timetable.Day2"
            android:label="@string/title_activity_day2" >
        </activity>
        <activity
            android:name="com.mini.timetable.Day3"
            android:label="@string/title_activity_day3" >
        </activity>
        <activity
            android:name="com.mini.timetable.Day4"
            android:label="@string/title_activity_day4" >
        </activity>
        <activity
            android:name="com.mini.timetable.Day5"
            android:label="@string/title_activity_day5" >
        </activity>
        <activity
            android:name="com.mini.timetable.Start"
            android:label="@string/title_activity_start" >
        </activity>
        <activity
            android:name="com.mini.timetable.Timetable"
            android:label="@string/title_activity_timetable" >
        </activity>
    </application>

</manifest>
